begin
  require 'rubygems'
rescue LoadError
  # no rubygems to load, so we fail silently
end

$:.unshift File.join(File.dirname(__FILE__), '..', 'lib')
require 'twitterscrobble'

begin
    prefs = Twitterscrobble::Preferences.new
    Twitterscrobble::TwitterScrobbler.new(prefs).update
    prefs.save
#rescue
#    $stderr.puts "\nError: #{$!}"
end
